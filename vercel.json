{
  "version": 2,
  "rewrites": [
    {
      "source": "/build/(.*)",
      "destination": "/public/build/$1"
    },
    {
      "source": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|mp4))",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ],
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}